@using CommonFx.ConfigGroups
@using CommonFx.Web.Mvc
@model dynamic

@{
    ViewBag.Title = this.VirtualPath;
    IList<ConfigGroup> configGroups = ViewBag.ConfigGroups;
}

<h2>@ViewBag.Title</h2>
<hr/>
<p>@configGroups.ToJsonRaw()</p>
<hr />
@foreach (var item in configGroups)
{
    <h2>@item.ConfigName</h2>
    foreach (var entry in item.Entries.Values)
    {
        <p>@entry.Desc : @entry.Value</p>
    }
}

